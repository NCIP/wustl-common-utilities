<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="edu.wustl.common.labelSQLApp.domain.LabelSQLAssociation"
		table="LABELSQLASSOC">
		<id name="id" type="long" length="30" column="IDENTIFIER">
			<generator class="native">
                <param name="sequence">LABELSQLASSOC_IDENTIFIER_SEQ</param>
            </generator>
		</id>

		<!--
			<many-to-one name="labelSQLCollectionProtocol"
			class="labelSQLApp.domain.LabelSQLCollectionProtocol" cascade="none"
			update="true" insert="true" column="CPID" lazy="false" />
		-->
		<property name="labelSQLCollectionProtocol" type="long"
			update="true" insert="true" column="CPID" length="30" />

		<many-to-one name="labelSQL" class="edu.wustl.common.labelSQLApp.domain.LabelSQL"
			cascade="none" update="true" insert="true" column="LABELSQLID" lazy="false" />

		<property name="userDefinedLabel" type="string" update="true"
			insert="true" column="USER_DEFINED_LABEL" length="255" />

		<property name="seqOrder" type="integer" update="true"
			insert="true" column="SEQ_ORDER" length="10" />

	</class>
</hibernate-mapping>

