<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="edu.wustl.common.idgenerator.KeySequenceGenerator"
        table="KEY_SEQ_GENERATOR"
		lazy="true"
    >

    <cache usage="read-write" />

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">KEY_GENERATOR_SEQ</param>
            </generator>
        </id>

 		<property
            name="keyValue"
            type="string"
            update="true"
            insert="true"
            column="KEY_VALUE"
            length="50"
        />

         <property
            name="keySequenceId"
            type="string"
            update="true"
            insert="true"
            column="KEY_SEQUENCE_ID"
            length="50"
        />

         <property
            name="keyType"
            type="string"
            update="true"
            insert="true"
            column="KEY_TYPE"
            length="255"
        />

    </class>

</hibernate-mapping>
