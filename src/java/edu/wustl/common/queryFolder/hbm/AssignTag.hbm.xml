<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="edu.wustl.common.queryFolder.beans.AssignTag" table="assigntag">
		<id name="id" type="java.lang.Long" column="ID">
			<generator class="increment" />
		</id>
		<many-to-one name="tag" class="edu.wustl.common.queryFolder.beans.Tag"
			fetch="select">
			<column name="TAG_ID" not-null="true" />
		</many-to-one>
		<property name="objId" type="java.lang.Long" column="OBJ_ID"
			unique="true" />
		<property name="objType" type="java.lang.String" column="OBJ_TYPE"
			unique="true" />
	</class>
</hibernate-mapping>